
// Debug Configuration for WhatsNext
// This configuration is used for development and testing

// Build Settings
SWIFT_OPTIMIZATION_LEVEL = -Onone
SWIFT_COMPILATION_MODE = incremental
SWIFT_ACTIVE_COMPILATION_CONDITIONS = DEBUG AI_FEATURES
GCC_OPTIMIZATION_LEVEL = 0
GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1

// Code Signing
CODE_SIGN_IDENTITY = Apple Development
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = $(DEVELOPMENT_TEAM)

// Product
PRODUCT_NAME = WhatsNext Dev
PRODUCT_BUNDLE_IDENTIFIER = com.gauntletai.whatsnext.dev

// App Configuration
ENABLE_TESTABILITY = YES
ENABLE_BITCODE = NO
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym

// Warnings
SWIFT_TREAT_WARNINGS_AS_ERRORS = NO
GCC_TREAT_WARNINGS_AS_ERRORS = NO

// Environment Variables (from system env)
SUPABASE_URL = $(IOS_SUPABASE_URL)
SUPABASE_ANON_KEY = $(IOS_SUPABASE_ANON_KEY)

// Push Notifications Environment
APNS_ENVIRONMENT = development

